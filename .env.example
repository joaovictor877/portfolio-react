# Configuração do MySQL (Railway)
# Altere com suas credenciais do Railway
MYSQL_HOST=seu-host.railway.app:porta
MYSQL_USER=root
MYSQL_PASSWORD=sua_senha_aqui
MYSQL_DATABASE=portfolio

# Porta da API (Railway vai definir automaticamente)
PORT=3001
API_PORT=3001

# Logs
LOG_API=1

# Node Environment
NODE_ENV=production
